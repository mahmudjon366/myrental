# ==========================================
# RENTACLOUD FRONTEND ENVIRONMENT VARIABLES
# ==========================================

# API Configuration
# Development API URL
VITE_API_BASE=http://localhost:4000/api

# Production API URL (uncomment and configure for production)
# VITE_API_BASE=https://your-domain.com/api
# VITE_API_BASE=https://api.your-domain.com

# Application Configuration
VITE_APP_NAME=Rentacloud
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Ijara boshqaruv tizimi

# Build Configuration
# VITE_BUILD_TARGET=es2015
# VITE_BUILD_MINIFY=true

# Development Configuration
# Enable debug mode (shows console logs, error details)
# VITE_DEBUG=true
# VITE_DEV_TOOLS=true

# Performance Configuration
# VITE_CHUNK_SIZE_WARNING_LIMIT=500

# PWA Configuration (if implementing PWA features)
# VITE_PWA_ENABLED=false
# VITE_PWA_NAME=Rentacloud
# VITE_PWA_SHORT_NAME=Rentacloud

# Analytics Configuration (if needed)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_ANALYTICS_ENABLED=false

# Feature Flags
# VITE_FEATURE_REPORTS=true
# VITE_FEATURE_NOTIFICATIONS=false
# VITE_FEATURE_DARK_MODE=false

# UI Configuration
# VITE_DEFAULT_LANGUAGE=uz
# VITE_THEME=light
# VITE_CURRENCY=UZS

# Error Reporting (if using services like Sentry)
# VITE_SENTRY_DSN=https://your-sentry-dsn
# VITE_ERROR_REPORTING_ENABLED=false

# Social/Contact Information
# VITE_CONTACT_PHONE=+998919752757
# VITE_CONTACT_EMAIL=support@rentacloud.uz
# VITE_COMPANY_NAME=ProX Development Team